{
    "extends": "@eggjs/tsconfig",
    "compilerOptions": {
        "target": "ES2022",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "paths": {
            "@/module/*": [
                "app/module/*"
            ]
        },
        "baseUrl": "."
    }
}